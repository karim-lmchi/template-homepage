        <div class = 'blocItems'>
            <a href='#'>{{ item1 }}</a>
            <a href='#'>{{ item2 }}</a>
            <a href='#'>{{ item3 }}</a>
            <a href='#'>{{ item4 }}</a>
            <a href='#'>{{ item5 }}</a>
        </div>
    </div>
{% endmacro %}